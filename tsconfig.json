{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "react": [
        "./node_modules/@types/react"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@Pages/*": [
        "src/Pages/*"
      ],
      "@Global/*": [
        "src/Global/*"
      ],
      "@Src/*": [
        "src/*"
      ]
    }
  }
}